package p1nata.p1n;

import java.io.File;
import java.util.Arrays;
import java.util.List;

public class P1NIO {
	private final P1Nmain main;
	public P1NIO(P1Nmain main) {
		this.main = main;
	}
	public List<DoorInfo> readDoors(){
return		Arrays.asList(main.getFilesDir().listFiles().map(f -> readDoorInfo(f.getName()));
		}
	}
}
